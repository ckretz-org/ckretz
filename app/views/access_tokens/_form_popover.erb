<dialog open id="access_tokens_form">
  <div class="bg-white p-4 rounded-lg w-full max-w-md">
    <div class="text-center">
      <h2 class="text-gray-500 grow text-lg font-bold inline-block"><%= access_token.persisted? ? "Edit" : "New" %></h2>
    </div>
    <%= render partial: "form", locals: { access_token: access_token } %>
  </div>
</dialog>
